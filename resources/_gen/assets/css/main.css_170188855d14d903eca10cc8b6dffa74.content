:root{--sans:system-ui, -apple-system, BlinkMacSystemFont, "avenir next", avenir, helvetica, "helvetica neue", ubuntu, roboto, noto, "segoe ui", arial, sans-serif;--mono:San Francisco Mono, Monaco, "Consolas", "Lucida Console", "DejaVu Sans Mono", "Bitstream Vera Sans Mono", monospace;--serif:var(--sans);--theme-ui-colors-text:#111111;--theme-ui-colors-background:#FAFAFA;--theme-ui-colors-primary:#FF6501;--theme-ui-colors-muted:#e3e3e3;--theme-ui-colors-highlight:#e3e3e3;--baseline:1rem}body{font:1rem/1.6 var(--serif);color:var(--theme-ui-colors-text);-webkit-font-smoothing:antialiased;text-rendering:optimizeLegibility;font-weight:400;font-style:normal;margin:0 auto;max-width:640px;padding:0 1rem;letter-spacing:.02px;hyphens:auto}@media(max-width:600px){body{font-size:85%;max-width:100%;margin:0 auto}}h1{font-family:var(--sans);font-weight:800;font-size:2rem;line-height:1.25;letter-spacing:.004em;padding:0;margin-bottom:2rem}h2,h3,h4{font-family:var(--sans);font-weight:400;line-height:1.25}h2{font-size:1.5rem;letter-spacing:.009em}small{font-family:var(--sans)}ul,ol{padding-left:1rem}a{color:inherit;text-decoration:none}main a{border-bottom:1px solid var(--theme-ui-colors-text)}main a:hover{color:var(--theme-ui-colors-primary);border-bottom-color:var(--theme-ui-colors-primary)}header{font-family:var(--sans);align-items:center;max-width:auto;margin:0 auto;display:flex;flex-wrap:wrap;font-weight:600;justify-content:space-between;padding-top:1.5rem}header a{text-decoration:none;border-bottom:none}header a:hover{color:var(--theme-ui-colors-primary)}nav{font-family:var(--sans);display:block;list-style:none;margin-top:1rem}nav ul{list-style:none;padding:0;margin:0}nav li{font-weight:400;display:inline-block}nav li+li{margin-left:.375rem}nav a{text-decoration:none;border-bottom:none}nav a:hover{color:var(--theme-ui-colors-primary)}nav .active a{color:var(--theme-ui-colors-primary);text-decoration:underline}footer{font-family:var(--sans);text-align:center;border-top:2px solid var(--theme-ui-colors-text);font-size:.8rem;padding:1.5rem 0;margin-top:3rem}footer a{color:inherit;text-decoration:none;border-bottom:none}footer a:hover{color:var(--theme-ui-colors-primary)}footer div{font-size:1.5rem}pre{background:var(--theme-ui-colors-highlight);padding:1rem;overflow-x:auto;font-family:var(--mono);font-size:.9rem}code{font-family:var(--mono);font-size:.9rem}img{max-width:100%;height:auto;display:block;margin:2rem auto}figure{margin:2rem 0}figcaption{font-family:var(--sans);font-size:.75rem;line-height:1.25;margin-top:-.75rem;text-align:right}blockquote{font-style:italic;border-left:4px solid var(--theme-ui-colors-primary);padding-left:1.5rem;margin:2.5rem 0}.text_decoration_none{text-decoration:none;border-bottom:none}