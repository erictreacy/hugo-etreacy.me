:root{--sans:-apple-system, BlinkMacSystemFont, "avenir next", avenir, helvetica, "helvetica neue", ubuntu, roboto, noto, "segoe ui", arial, sans-serif;--mono:San Francisco Mono, Monaco, "Consolas", "Lucida Console", "DejaVu Sans Mono", "Bitstream Vera Sans Mono", monospace;--serif:var(--sans);--etreacy-ui-colors-primary:#FF6501}body{font:1rem/1.6 var(--serif);-webkit-font-smoothing:antialiased;text-rendering:optimizeLegibility;font-weight:400;font-style:normal;margin:0 auto;max-width:500px;padding:0 1rem;letter-spacing:.02px;hyphens:auto}@media(max-width:600px){body{font-size:85%;max-width:100%;margin:0 auto}}h1{font-size:32px;font-weight:600;line-height:1.25;letter-spacing:.004em;padding-top:1.25rem;margin-bottom:2rem}a{color:inherit;text-decoration:none}main a{border-bottom:1px solid var(--etreacy-ui-colors-text)}main a:hover{color:var(--etreacy-ui-colors-primary);border-bottom-color:var(--etreacy-ui-colors-primary)}header{align-items:center;max-width:auto;margin:0 auto;display:flex;flex-wrap:wrap;font-weight:600;justify-content:space-between;padding-top:1.5rem}header a{text-decoration:none;border-bottom:none}header a:hover{color:var(--etreacy-ui-colors-primary)}nav{display:none}footer{text-align:center;border-top:2px solid var(--etreacy-ui-colors-text);font-size:.8rem;padding:1.5rem 0;margin-top:3rem}footer a{color:inherit;text-decoration:none;border-bottom:none}footer a:hover{color:var(--etreacy-ui-colors-primary)}footer div{font-size:1.5rem}.border{border:4px double var(--etreacy-ui-colors-text)}.my0{margin-top:0;margin-bottom:0}.mt3{margin-top:3rem}.mb2{margin-bottom:2rem}.mb3{margin-bottom:3rem}.p2{padding:1.5rem}